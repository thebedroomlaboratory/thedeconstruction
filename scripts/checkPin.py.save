from time import sleep
import sys
import RPi.GPIO as GPIO
GPIO.setmode(GPIO.BCM)
pin=int(sys.argv[1])
i=int(sys.argv[2])
print pin
GPIO.setup(pin, GPIO.OUT)
    

while 
	if i==1:
    		GPIO.output(pin, True)
	elif i==0:
		GPIO.output(pin, False)
	else:
		print 'Invalid current. should be 1 or 0'

sys.exit(0)
